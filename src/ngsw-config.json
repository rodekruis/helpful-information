{
  "$schema": "../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.json",
          "/*.css",
          "/*.js",
          "!/*-es5*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "googleSheetsApi",
      "version": 4,
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 42,
        "maxAge": "7d",
        "timeout": "8s"
      },
      "urls": ["https://sheets.googleapis.com/v4/spreadsheets/*"]
    }
  ]
}
